<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.vis.accountingvis.mapper.FinancialIndicatorMapper">
    <select id="selectByIndicatorName" resultType="com.vis.accountingvis.entity.FinancialIndicator">
        SELECT * FROM financial_indicators
        WHERE Indicator_Name = #{indicatorName}
    </select>
</mapper>